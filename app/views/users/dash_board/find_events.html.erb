

<%= render partial: "layouts/dash_board/sidebar_top"%>

<div class="col-custom-md-21">
      <div class="content-section" style="height:600px">
      <h2 class="title">DASHBOARD
      </h2>
      <div class="about-box-head">
        <form accept-charset="UTF-8" action="/find_events" method="get" class="form-inline">
              <span class="title-2">Find  <span class="highlight-1">Event</span></span>
            
            
            <p>  <label class="control-label" for="inputpagetitle">SEARCH EVENTS: </label>
               
              <input class="control-label" id="search" name="search" type="text" />
              <%= submit_tag "Search", :name => nil, :class=>"btn-sm new" %> 
            </p>  
        <%# end %> 
        </form> 
        </div>
<table class="table table-striped table-bordered table-condensed">
  <tr>
    <th>Title</th>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
    <th>Description</th>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
    <th>Venue</th>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
    <th>Event start date</th>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
    <th>Event end date</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
<% if @events%>
<% @events.each do |event| %>
  <tr>
    <td><%= event.title %></td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
    <td><%= event.description %></td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
    <td><%= event.venue %></td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
    <td><%= event.event_start_date %></td>&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;
    <td><%= event.event_end_date %></td>

  </tr>
<% end %>
<% end %>
</table>

<br />
<div class="clearfix">
       
<div>

<%= render partial: "layouts/dash_board/sidebar_bottom"%>

      
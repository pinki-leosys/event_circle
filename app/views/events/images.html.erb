<% @images.each do |image| %>
 <%= link_to image_tag( image.url, :height => '120', :weight => '160'),image.url,  {:class => 'fancybox-group', rel: "group1"} %>
<% end %>

<script>
$("a.fancybox-group").fancybox({
		'transitionIn'	:	'elastic',
		'transitionOut'	:	'elastic',
		'speedIn'		:	600, 
		'speedOut'		:	200, 
		'overlayShow'	:	false
	});</script>